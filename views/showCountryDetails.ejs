<table>
    <p><a href="<%= countryLink %>" class="btn btn-primary"><input type="submit" class="btn btn-primary" value="Go back"></input></a></p>
    <tr>
        <th>Country Code</th>
        <th>Country Name</th>
        <th>Country details</th>
        
    </tr>   
    <% countries.forEach((country) => { %>
    <tr>
    <td><%=country.co_code%></td>
    <td><%=country.co_name%></td>
    <td><%=country.co_details%></td>
</tr>
<%})%>
</table>

